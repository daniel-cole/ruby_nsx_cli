<interfaces>
  <interface>
    <name><%= name %></name>
    <addressGroups>
      <addressGroup>
        <primaryAddress><%= primary_address %></primaryAddress>
        <subnetMask><%= subnet_mask %></subnetMask>
      </addressGroup>
    </addressGroups>
    <mtu><%= mtu %></mtu>
    <type><%= type %></type>
    <isConnected>true</isConnected>
    <connectedToId><%= connected_to_id %></connectedToId>
  </interface>
</interfaces>
